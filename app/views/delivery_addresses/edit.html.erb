<h2>配送先編集</h2>

<%= form_for(@deliver) do |f| %>
<div class="field row">
    <%= f.text_field :postal_code %>
</div>

<div class="field row">
    <%= f.label :住所 %>
    <%= f.text_field :address %>
</div>

<div class="field row">
    <%= f.label :宛名 %>
    <%= f.text_field :name_address %>
</div>
<div class="actions row">
    <%= f.submit '編集する' %>
</div>
<% end %>